{
  "name": "setbased/helper-code-store-php",
  "description": "Helper class for indenting automatically generated PHP code",
  "keywords": [
    "code",
    "generation",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "setbased/helper-code-store": "^2.5.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Helper\\CodeStore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Helper\\CodeStore\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
