{
  "name": "setbased/helper-code-store-php",
  "description": "Helper class for indenting automatically generated PHP code",
  "keywords": [
    "code",
    "generation",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "setbased/helper-code-store": "^2.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "phing/phing": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Helper\\CodeStore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Helper\\CodeStore\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
