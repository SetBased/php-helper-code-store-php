{
  "name": "setbased/helper-code-store-php",
  "description": "Helper class for indenting automatically generated PHP code",
  "keywords": [
    "code",
    "generation",
    "php"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "setbased/helper-code-store": "^2.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "phing/phing": "^2.16.3"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Helper\\CodeStore\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Helper\\CodeStore\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
